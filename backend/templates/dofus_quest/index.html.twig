{% extends 'base.html.twig' %}

{% block title %}DofusQuest index{% endblock %}

{% block body %}
    <h1>DofusQuest index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>QuestOrder</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for dofus_quest in dofus_quests %}
            <tr>
                <td>{{ dofus_quest.id }}</td>
                <td>{{ dofus_quest.questOrder }}</td>
                <td>
                    <a href="{{ path('app_dofus_quest_show', {'id': dofus_quest.id}) }}">show</a>
                    <a href="{{ path('app_dofus_quest_edit', {'id': dofus_quest.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_dofus_quest_new') }}">Create new</a>
{% endblock %}
